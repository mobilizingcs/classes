                    <id>social_media</id>
                    <displayLabel>social_media</displayLabel>
                    <promptText>How many MINUTES did you spend on social media?</promptText>
                    <default>0</default>
                    <condition>activities == 17</condition>
                    <skippable>false</skippable>
                    <properties>
