                <prompt>
                    <promptType>number</promptType>
                    <id>read</id>
                    <displayLabel>Read</displayLabel>
                    <promptText>How many MINUTES did you spend reading (not for class)?</promptText>
                    <default>0</default>
                    <condition>activities == 11</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>communicate</id>
                    <displayLabel>communicate</displayLabel>
                    <promptText>How many MINUTES did you spend communicating (includes texting, emails, video and voice calls)?</promptText>
                    <default>0</default>
                    <condition>activities == 12</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>socialize</id>
                    <displayLabel>socialize</displayLabel>
                    <promptText>How many MINUTES did you spend socializing (outside of class, in person)?</promptText>
                    <default>0</default>
                    <condition>activities == 13</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>spiritual</id>
                    <displayLabel>spiritual</displayLabel>
                    <promptText>How many MINUTES did you spend on a spiritual activity?</promptText>
                    <default>0</default>
                    <condition>activities == 14</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>purchases</id>
                    <displayLabel>purchases</displayLabel>
                    <promptText>How many MINUTES did you spend purchasing items online or in a store?</promptText>
                    <default>0</default>
                    <condition>activities == 15</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>extra</id>
                    <displayLabel>extra</displayLabel>
                    <promptText>How many MINUTES did you spend on hobbies/volunteering/leisure/extra-curricular activities (excluding sports and physical activity)?</promptText>
                    <default>0</default>
                    <condition>activities == 16</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
