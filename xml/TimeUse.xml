<?xml version="1.0" encoding="UTF-8"?>
<campaign>
    <surveys>
        <survey>
            <id>timeuse</id>
            <title>Time Use</title>
            <description>This survey asks you questions about the activities you've participated in during a 5 day period.</description>
            <submitText>Thank you for filling out the survey!</submitText>
            <anytime>true</anytime>
            <contentList>
                <prompt>
                    <promptType>number</promptType>
                    <id>day</id>
                    <displayLabel>Day</displayLabel>
                    <promptText>For which day are you collecting data?</promptText>
                    <default>1</default>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>1</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>10</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>multi_choice</promptType>
                    <id>activities</id>
                    <displayLabel>Activities</displayLabel>
                    <promptText>What activities did you participate in?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>0</key>
                            <label>Sleep</label>
                        </property>
                        <property>
                            <key>1</key>
                            <label>Meals/Eating</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>School</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>Homework</label>
                        </property>
                        <property>
                            <key>4</key>
                            <label>Work</label>
                        </property>
                        <property>
                            <key>5</key>
                            <label>Grooming</label>
                        </property>
                        <property>
                            <key>6</key>
                            <label>Travel/Commuting</label>
                        </property>
                        <property>
                            <key>7</key>
                            <label>Chores</label>
                        </property>
                        <property>
                            <key>8</key>
                            <label>Television</label>
                        </property>
                        <property>
                            <key>9</key>
                            <label>Videogames</label>
                        </property>
                        <property>
                            <key>10</key>
                            <label>Sports</label>
                        </property>
                        <property>
                            <key>11</key>
                            <label>Reading</label>
                        </property>
                        <property>
                            <key>12</key>
                            <label>Communicating</label>
                        </property>
                        <property>
                            <key>13</key>
                            <label>Socializing</label>
                        </property>
                        <property>
                            <key>14</key>
                            <label>Spiritual</label>
                        </property>
                        <property>
                            <key>15</key>
                            <label>Purchasing</label>
                        </property>
                        <property>
                            <key>16</key>
                            <label>Extracurricular</label>
                        </property>
                        <property>
                            <key>17</key>
                            <label>Social Media</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>sleep</id>
                    <displayLabel>Sleep</displayLabel>
                    <promptText>How many MINUTES did you sleep?</promptText>
                    <default>0</default>
                    <condition>activities == 0</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>meals</id>
                    <displayLabel>Meals</displayLabel>
                    <promptText>How many MINUTES did you spend eating/drinking?</promptText>
                    <default>0</default>
                    <condition>activities == 1</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>school</id>
                    <displayLabel>School</displayLabel>
                    <promptText>How many MINUTES did you spend in classes at school?</promptText>
                    <default>0</default>
                    <condition>activities == 2</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>homework</id>
                    <displayLabel>Homework</displayLabel>
                    <promptText>How many MINUTES did you spend doing homework?</promptText>
                    <default>0</default>
                    <condition>activities == 3</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>work</id>
                    <displayLabel>Working</displayLabel>
                    <promptText>How many MINUTES did you spend working at a job?</promptText>
                    <default>0</default>
                    <condition>activities == 4</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>grooming</id>
                    <displayLabel>Grooming</displayLabel>
                    <promptText>How many MINUTES did you spend grooming yourself?</promptText>
                    <default>0</default>
                    <condition>activities == 5</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>travel</id>
                    <displayLabel>Travel</displayLabel>
                    <promptText>How many MINUTES did you spend traveling/commuting?</promptText>
                    <default>0</default>
                    <condition>activities == 6</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>chores</id>
                    <displayLabel>Chores</displayLabel>
                    <promptText>How many MINUTES did you spend doing household chores?</promptText>
                    <default>0</default>
                    <condition>activities == 7</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>television</id>
                    <displayLabel>Television</displayLabel>
                    <promptText>How many MINUTES did you spend watching television (including streaming)?</promptText>
                    <default>0</default>
                    <condition>activities == 8</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>videogames</id>
                    <displayLabel>Videogames</displayLabel>
                    <promptText>How many MINUTES did you spend playing video games?</promptText>
                    <default>0</default>
                    <condition>activities == 9</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>sports</id>
                    <displayLabel>Sports</displayLabel>
                    <promptText>How many MINUTES did you spend participating in sports/exercise/physical activity?</promptText>
                    <default>0</default>
                    <condition>activities == 10</condition>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>1440</label>
                        </property>
                        <property>
