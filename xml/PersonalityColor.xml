<?xml version="1.0" encoding="UTF-8"?>
<campaign>
    <surveys>
        <survey>
            <id>personalitycolor</id>
            <title>Personality Color</title>
            <description>Share the data from the results of your True Colors personality test plus other information about you.</description>
            <submitText>Click on the green button to submit your data.</submitText>
            <anytime>true</anytime>
            <contentList>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>int_ext</id>
                    <displayLabel>Personality Type</displayLabel>
                    <promptText>Are you mostly an introvert (turn to your own mind to recharge) or an extrovert (seek other people for energy)?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>0</key>
                            <label>Introvert</label>
                        </property>
                        <property>
                            <key>1</key>
                            <label>Extrovert</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>p_color</id>
                    <displayLabel>Predominant Color</displayLabel>
                    <promptText>Enter your predominant personality color.</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>0</key>
                            <label>Gold</label>
                        </property>
                        <property>
                            <key>1</key>
                            <label>Green</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>Orange</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>Blue</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>s_color</id>
                    <displayLabel>Secondary Color</displayLabel>
                    <promptText>Enter your secondary personality color.</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>0</key>
                            <label>Gold</label>
                        </property>
                        <property>
                            <key>1</key>
                            <label>Green</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>Orange</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>Blue</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>orange</id>
                    <displayLabel>Total orange score</displayLabel>
                    <promptText>Enter the total score for orange.</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>5</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>20</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>green</id>
                    <displayLabel>Total green score</displayLabel>
                    <promptText>Enter the total score for green.</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>5</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>20</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>gold</id>
                    <displayLabel>Total gold score</displayLabel>
                    <promptText>Enter the total score for gold.</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>5</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>20</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>blue</id>
                    <displayLabel>Total blue score</displayLabel>
                    <promptText>Enter the total score for blue.</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>5</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>20</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>sport</id>
                    <displayLabel>Sports activity</displayLabel>
                    <promptText>Are you involved in sports?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>0</key>
                            <label>Yes</label>
                        </property>
                        <property>
                            <key>1</key>
                            <label>No</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>SuperPower</id>
                    <displayLabel>Super Power</displayLabel>
                    <promptText>Which of the super powers would you most like to have? </promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>0</key>
                            <label>Invisibility</label>
                        </property>
                        <property>
                            <key>1</key>
                            <label>Telepathy (read minds)</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>Freeze Time</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>Super Strength</label>
                        </property>
                        <property>
                            <key>4</key>
                            <label>Fly</label>
                        </property>
                    </properties>
                </prompt>
            </contentList>
        </survey>
    </surveys>
</campaign>
