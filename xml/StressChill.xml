<campaign>
    <surveys>
        <survey>
            <id>stresschill</id>
            <title>Stress Chill</title>
            <description>Measure your stress/chill level</description>
            <submitText>Click on the green button to submit your data.</submitText>
            <anytime>true</anytime>
            <contentList>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>where</id>
                    <displayLabel>Where</displayLabel>
                    <promptText>Where are you?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>0</key>
                            <label>school</label>
                        </property>
                        <property>
                            <key>1</key>
                            <label>work</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>home</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>public place</label>
                        </property>
                        <property>
                            <key>4</key>
                            <label>other's house</label>
                        </property>
                        <property>
                            <key>5</key>
                            <label>commuting</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>text</promptType>
                    <id>why</id>
                    <displayLabel>Why</displayLabel>
                    <promptText>Why are you here (in one word)?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>3</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>200</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>howmanypeople</id>
                    <displayLabel>How many people?</displayLabel>
                    <promptText>How many people are you with (not counting yourself)?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>10000</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>who</id>
                    <displayLabel>Who</displayLabel>
                    <promptText>Who are you with?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>0</key>
                            <label>alone</label>
                        </property>
                        <property>
                            <key>1</key>
                            <label>friends and/or family</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>teachers and classmates</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>strangers</label>
                        </property>
                        <property>
                            <key>4</key>
                            <label>coworkers</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>stress</id>
                    <displayLabel>Stress</displayLabel>
                    <promptText>How stressed are you feeling right now (3 is very stressed, 0 not stressed at all)?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>3</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>true</label>
                        </property>
                    </properties>
                </prompt>
            </contentList>
        </survey>
    </surveys>
</campaign>
